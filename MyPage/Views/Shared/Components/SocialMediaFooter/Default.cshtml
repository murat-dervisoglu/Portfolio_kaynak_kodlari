@using EntityLayer.Concrete
@model List<SocialMedia>

<div class="mt-3">
    <h3 class="h6">Diğer Adresler</h3>
    <ul class="nav justify-content-start gap-2">

        @foreach (var item in Model)
        {
            <li class="nav-item">
                <a class="nav-link" href="@item.Account" title="@item.Name" target="_blank">
                    <i class="@item.Icon fa-lg"></i>
                </a>
            </li>
        }
    </ul>
</div>
